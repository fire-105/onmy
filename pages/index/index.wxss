/**index.wxss**/
.container {
	display: flex;
	flex-direction: column;
	flex-wrap: nowrap;
	align-items: center;
    color: #424242;
}
.popup_btn {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 40rpx;
  margin: 10rpx;
	width: calc(100% - 20rpx);
  box-sizing: border-box;
	background-color: #f7f8fa;
  border-radius: 100rpx;
}
